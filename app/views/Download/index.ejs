<div class="container-fluid">
  <div class="row">
    <% dep_data.forEach(data=>{
        %>
    <div class="col-lg-3 col-md-4 col-sm-6 col-12 ">
      <div class="thumbnail">
        <a href="/download/<%=data._id%>">
          <img src="images/<%=data.dep_img%>" onerror="altImg(this)" alt="<%=data.dep_name%>"
            style="width:100%; height: 200px">
          <div class="caption">
            <p><%=data.dep_name%></p>
          </div>
        </a>
      </div>
    </div>
    <% })%>
  </div>
</div>